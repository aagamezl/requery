{
  "compilerOptions": {
  "allowJs": true,
  "allowSyntheticDefaultImports": true,
  "alwaysStrict": true,
  "checkJs": true,
  "esModuleInterop": true,
  "lib": [
    "ESNext"
  ],
  "module": "NodeNext",
  "noEmit": true,
  "noImplicitThis": true,
  "strict": true,
  "strictNullChecks": true,
  "target": "ESNext"
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "test",
    "src"
  ]
}
